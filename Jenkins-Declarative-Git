pipeline{
	agent any

	environment{
		PATH = "/opt/maven-3.6.3/bin:$PATH"
	}	
	stages{
		stage("Git Checkout"){
			steps{
				sh 'https://github.com/rameshsampathi/pipelinetesting.git'
			}    
		}
		stage("Maven Build"){
			steps{
				sh 'mvn clean package'
			}
		}           
	}
}                  
